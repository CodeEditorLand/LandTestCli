{
	"name": "@vscode/test-cli",
	"version": "0.0.1",
	"private": false,
	"description": "Command-line runner for VS Code extension tests",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./out/index.mjs",
			"require": "./out/index.cjs"
		},
		"./fullJsonStream": {
			"require": "./out/fullJsonStreamReporter.cjs"
		}
	},
	"main": "out/index.cjs",
	"bin": {
		"vscode-test": "./out/bin.mjs"
	},
	"scripts": {
		"build": "npm run clean && tsc",
		"clean": "node -e \"fs.rmSync('out',{force:true,recursive:true})\"",
		"prepack": "npm run build",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"prettier": "prettier --write src",
		"watch": "tsc --watch"
	},
	"dependencies": {
		"@types/mocha": "10.0.6",
		"c8": "9.1.0",
		"chokidar": "3.6.0",
		"enhanced-resolve": "5.16.0",
		"glob": "10.3.10",
		"minimatch": "9.0.3",
		"mocha": "10.3.0",
		"supports-color": "9.4.0",
		"yargs": "17.7.2"
	},
	"devDependencies": {
		"@types/node": "20.11.30",
		"@types/yargs": "17.0.32",
		"@vscode/test-electron": ">=2",
		"@vscode/test-web": ">=0.0.52",
		"typescript": "5.4.2",
		"typescript-esbuild": "0.4.5"
	},
	"publishConfig": {
		"access": "public"
	}
}
