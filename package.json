{
	"name": "@vscode/test-cli",
	"version": "0.0.1",
	"private": false,
	"description": "Command-line runner for VS Code extension tests",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandTestCli#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandTestCli/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandTestCli.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./out/index.mjs",
			"require": "./out/index.cjs"
		},
		"./fullJsonStream": {
			"require": "./out/fullJsonStreamReporter.cjs"
		}
	},
	"main": "out/index.cjs",
	"bin": {
		"vscode-test": "./out/bin.mjs"
	},
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"build": "npm run clean && tsc",
		"clean": "node -e \"fs.rmSync('out',{force:true,recursive:true})\"",
		"prepack": "npm run build",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"dependencies": {
		"c8": "9.1.0",
		"chokidar": "3.6.0",
		"enhanced-resolve": "5.16.0",
		"glob": "10.3.12",
		"minimatch": "9.0.4",
		"supports-color": "9.4.0",
		"yargs": "17.7.2"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.12.10",
		"@types/yargs": "17.0.32"
	},
	"publishConfig": {
		"access": "public"
	}
}
